            ]
        },
        "ext.liverc.rc": {
            "scripts": [
                "modules/rc/mw.liverc.rc.js",
                "modules/rc/mw.liverc.rc.Generic.js",
                "modules/rc/mw.liverc.rc.Edit.js",
                "modules/rc/mw.liverc.rc.Log.js"
